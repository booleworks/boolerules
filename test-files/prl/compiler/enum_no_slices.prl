header {
  prl_version 1.0
}

enum feature a ["a1", "a2"]
enum feature b ["b1", "b2", "b3"]
enum feature c ["c1", "c2"]
enum feature p ["p1", "p2", "p3"]
enum feature `q r` ["q1", "q2"]

rule [a = "a2"]
rule if [a = "a2"]  then [b = "b1"]
rule [p in ["p1", "p2"]]
rule if [`q r` in ["q1"]] then [p = "p1"]
