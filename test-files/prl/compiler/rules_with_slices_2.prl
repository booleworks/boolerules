header {
  prl_version 1.0
}

slicing properties {
  int version
  enum release
}

feature f1 {
  version [1-20]
  release ["R1"]
}

feature f2 {
  version [1, 3, 5, 7, 9, 11, 13]
  release ["R1"]
}

feature f3 {
  release ["R3", "R4"]
}

feature f4 {
  release ["R3", "R4"]
}

rule f1 => f2 {
  version [1-20]
  release ["R1"]
}

rule f2 => f3 {
  version [1-20]
}

rule f3 => f4 {
  version [1, 3, 5, 7, 9, 11, 13]
}

rule f4 => f1 {
  version [1, 3, 5, 7, 9, 11, 13]
  release ["R1"]
}

rule f4 => f2 {
  version [1, 3, 5, 7, 9, 11, 13]
  release ["R3", "R4"]
}
